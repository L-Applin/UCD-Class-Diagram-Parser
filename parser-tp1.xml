<?xml version="1.0" encoding="UTF-8"?>
<project name="parser-tp1" default="all">

    <!-- Reset folders -->
    <target name="clean" description="remove classes and jar">
        <delete dir="release/classes"/>
    </target>

    <!-- Remove extra folders TODO: remove if not used eventually -->
    <target name="cleanExtra" description="remove folders generated from code">
        <!--<delete dir="fichiersComptable"/>-->
        <!--<delete dir="repertoireServices"/>-->
    </target>

    <!-- Pour compiler les .JAVA en .CLASS vers un dossier "classes" -->
    <target name="compile"
            description="compile the Java source code to class files">
        <mkdir dir="release/classes"/>
        <javac srcdir="src" destdir="release/classes"/>
    </target>

    <!-- Pour créer les folders "release/Doc" -->
    <target name="setup" description="set up the proper folders for JavaDoc">
        <delete dir="release/Documentation"/>
        <mkdir dir="release/Documentation"/>
    </target>

    <!-- TODO: Pour créer "tp1.jar" -->
    <target name="jar" depends="compile"
            description="create a Jar file for the application">
        <jar destfile="release/tp1.jar" update="true">
            <fileset dir="release/classes" includes="**/*.class"/>
            <manifest>
                <attribute name="Main-Class" value="sample.Main"/>
            </manifest>
        </jar>
    </target>

    <!-- Generate JavaDoc -->
    <target name="doc" depends="setup" description="generate documentation">
        <javadoc sourcepath="src" destdir="release/Documentation"/>
    </target>

    <!-- Pour tout exécuter en même temps, dans le bon ordre -->
    <target name="all" depends="clean,cleanExtra,jar,doc"
            description="to run it all in the proper order" />


